<!-- Navigation Menu including Bread Crumb Menu
================================================== -->
<%= render 'shared/breadcrumbs' %>

    <div class="inner">
      <table class="table table-striped table-bordered">
        <tr>
          <th class="first">ID</th>
          <th><%= t("activerecord.attributes.publicity/qa_category.name", :default => t("activerecord.labels.name", :default => "Name")) %></th>
          <th><%= t("action.created_at", :default => "Created at")  %></th>
          <th width="180">Action</th>
        </tr>
        <% @qa_categories.each do |qa_category| -%>
        <tr>
          <td><%= qa_category.id %></td>
          <td><%= link_to qa_category.name, publicity_qa_category_path(qa_category) %></td>
          <td><%= qa_category.created_at %></td>
          <td class="last">
	        <%= show_button qa_category %> <%= edit_button qa_category %>
          </td>
        </tr>
        <% end -%>
      </table>
      </div>
